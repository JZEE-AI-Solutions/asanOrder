{
  "watch": [
    "routes",
    "services",
    "middleware",
    "lib",
    "utils",
    "server.js"
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/**",
    "tests/**",
    "*.test.js",
    "*.spec.js"
  ],
  "delay": 1000,
  "env": {
    "NODE_ENV": "development"
  },
  "verbose": true
}
